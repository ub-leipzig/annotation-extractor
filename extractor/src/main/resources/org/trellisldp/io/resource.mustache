<!DOCTYPE html>
<html>
    <head>
      <title>{{title}}</title>
      <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
      {{#icon}}
      <link rel="icon" href="{{icon}}"/>
      {{/icon}}
      {{#css}}
      <link rel="stylesheet" type="text/css" href="{{.}}"/>
      {{/css}}
    </head>
    <body>
      <header id="banner"><h1>{{title}}</h1></header>
      <main>
      {{#triples}}
      <p>
        {{subject}} <a href="{{predicate}}">{{predicateLabel}}</a>
        {{#objectIsIRI}}
          <a href="{{object}}">{{objectLabel}}</a>
        {{/objectIsIRI}}
        {{^objectIsIRI}}
          {{objectLabel}}
        {{/objectIsIRI}}
      </p>
      {{/triples}}
      </main>
      <footer><a href="http://www.trellisldp.org">Trellis Repository</a></footer>
      {{#js}}
      <script src="{{.}}"></script>
      {{/js}}
    </body>
</html>
